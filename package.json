{
  "name": "bh-web-frontend",
  "version": "0.0.1",
  "description": "Frontend for various Book of Hours experiments",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build:wp": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "build": "cross-env NODE_ENV=production vite build",
    "preview": "cross-env NODE_ENV=development vite preview",
    "test": "jest",
    "start:wp": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.js",
    "start": "cross-env NODE_ENV=development vite",
    "watch-errors": "tsc --noEmit --watch"
  },
  "private": true,
  "keywords": [
    "Book of Hours"
  ],
  "author": "RoboPhred",
  "license": "ISC",
  "devDependencies": {
    "@types/events": "^3.0.0",
    "@types/history": "^5.0.0",
    "@types/lodash": "^4.14.197",
    "@types/luxon": "^3.3.1",
    "@types/platform": "^1.3.4",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/webpack-env": "^1.17.0",
    "@vitejs/plugin-react": "^4.0.4",
    "cross-env": "^6.0.3",
    "rimraf": "^3.0.0",
    "typescript": "^4.7.4",
    "vite": "^4.4.9",
    "vite-plugin-checker": "^0.6.2"
  },
  "dependencies": {
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.14.6",
    "@mui/material": "^5.8.2",
    "@mui/x-data-grid": "^6.12.0",
    "classnames": "^2.3.1",
    "events": "^3.3.0",
    "history": "^5.3.0",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.2",
    "microinject": "^0.12.0",
    "platform": "^1.3.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "rxjs": "^7.5.5",
    "scheduler": "^0.23.0",
    "secrethistories-api": "^1.10.0",
    "type-fest": "^2.13.0",
    "use-context-selector": "^1.4.1"
  }
}
